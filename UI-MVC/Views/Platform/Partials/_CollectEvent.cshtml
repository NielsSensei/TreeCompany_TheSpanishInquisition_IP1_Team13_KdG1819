@model Domain.Users.Event

<div class="card">
    <div class="card-body">
        <p class="card-title">@Model.Name</p>
        <p>@Model.Description</p>
    </div>
    <div class="card-footer">
        <p><span style="font-weight: bold">Start </span>@Model.StartDate.ToShortDateString() | <span style="font-weight: bold">End </span>
            @Model.EndDate.ToShortDateString()</p>
    </div>
</div>