@model Domain.UserInput.MapField
@inject UIMVC.Settings Settings

<div class="embed-responsive embed-responsive-1by1">
    <iframe class="embed-responsive-item" src="https://www.google.com/maps/embed/v1/view?key=@Settings.GetApiKey()
                   &center=@Model.LocationX.ToString().Replace(",","."),
    @Model.LocationY.ToString().Replace(",",".")&zoom=20"></iframe>
</div>
